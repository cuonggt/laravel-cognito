{
    "name": "cuonggt/laravel-cognito",
    "description": "AWS Cognito auth driver for Laravel.",
    "keywords": [
        "laravel",
        "aws",
        "cognito"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Giang Thai Cuong",
            "email": "thaicuong.giang@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "aws/aws-sdk-php": "^3.74",
        "illuminate/auth": "~5.5",
        "illuminate/support": "~5.5"
    },
    "require-dev": {
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "Cuonggt\\LaravelCognito\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cuonggt\\LaravelCognito\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Cuonggt\\LaravelCognito\\LaravelCognitoServiceProvider"
            ],
            "aliases": {
                "CognitoClient": "Cuonggt\\LaravelCognito\\CognitoClient"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
